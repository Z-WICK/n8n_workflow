# ===========================================
# n8n Docker Compose Environment Variables
# ===========================================
# Copy this file to .env and fill in your values

# ----- Domain Configuration -----
# The top level domain to serve from
DOMAIN_NAME=example.com

# The subdomain to serve from (n8n will be accessible at https://SUBDOMAIN.DOMAIN_NAME)
SUBDOMAIN=n8n

# ----- SSL Configuration -----
# The email address for Let's Encrypt SSL certificate
SSL_EMAIL=your-email@example.com

# ----- Timezone -----
# Timezone for Cron and scheduling nodes
GENERIC_TIMEZONE=Asia/Shanghai

# ----- Feishu Integration (Optional) -----
# Remove or leave empty if not using Feishu
FEISHU_BASE_URL=https://open.feishu.cn
FEISHU_APP_ID=your_feishu_app_id
FEISHU_APP_SECRET=your_feishu_app_secret
FEISHU_APP_TOKEN=your_feishu_app_token
FEISHU_TABLE_ID=your_feishu_table_id
